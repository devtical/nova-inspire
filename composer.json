{
    "name": "kristories/nova-inspire",
    "description": "The best way to connect with your customers is by reaching out and inspiring them",
    "keywords": [
        "laravel",
        "nova",
        "inspire",
        "inspiring",
        "card"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^6.24|^7.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kristories\\Inspire\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kristories\\Inspire\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
